{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"types": ["bun-types"],
		"strict": true,
		"skipLibCheck": true,
		"baseUrl": ".",
		"declaration": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "dist",
		"paths": {
			"elysia-atlas/*": ["./src/*"]
		}
	},
	"include": ["src"]
}
