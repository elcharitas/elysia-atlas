{
    "name": "elysia-atlas",
    "version": "1.0.1",
    "description": "Strictly typed file-system router and autoloader for ElysiaJS",
    "author": {
        "name": "Jonathan Irhodia",
        "url": "https://github.com/elcharitas"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/elcharitas/elysia-atlas.git"
    },
    "keywords": [
        "elysia",
        "autoload",
        "fs",
        "router",
        "bun"
    ],
    "bugs": {
        "url": "https://github.com/elcharitas/elysia-atlas/issues"
    },
    "homepage": "https://github.com/elcharitas/elysia-atlas#readme",
    "type": "module",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.js",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./typegen": {
            "types": "./dist/typegen.d.ts",
            "import": "./dist/typegen.js"
        },
        "./types": {
            "types": "./dist/types.d.ts"
        }
    },
    "peerDependencies": {
        "elysia": ">=1.0.0"
    },
    "devDependencies": {
        "@elysiajs/eden": "^1.4.6",
        "@types/bun": "^1.3.8",
        "bun-types": "^1.3.8",
        "elysia": "^1.4.22",
        "typescript": "^5.9.3"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rm -rf dist && bun build ./src/index.ts ./src/typegen.ts --outdir ./dist --target bun --minify --splitting --external elysia && tsc --project tsconfig.json --emitDeclarationOnly",
        "prepublishOnly": "bun test && bun run build",
        "test": "bun test"
    }
}
